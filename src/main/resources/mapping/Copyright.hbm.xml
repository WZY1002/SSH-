<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 用于声明 实例类和表的映射关系
    声明 属性和表中列(字段)的对应关系 -->
    <class name="com.pl.model.CopyrightEntity"
           table="copyright">
        <!-- id标签用于映射主键列 -->
        <id name="id" column="id"></id>
        <!-- property标签用于映射普通属性 -->
        <property name="workname" column="workname"/>
        <property name="worktype" column="worktype"/>
        <property name="author" column="author"/>
        <property name="copyrighter" column="copyrighter"/>
        <property name="completetime" column="completetime"/>
        <property name="publishtime" column="publishtime"/>
        <property name="registration" column="registration"/>
        <property name="registid" column="registid"/>
        <property name="access" column="access"/>
        <property name="status" column="status"/>
        <property name="tid" column="tid"/>
    </class>
</hibernate-mapping>